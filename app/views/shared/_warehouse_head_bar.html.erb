<div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
          <div class="ibox-content" style="display: block;">
            <div class="row">
              <div class="col-sm-12">
                <h3 class="m-t-none m-b"><strong>Almacén:</strong> <%= @warehouse.name %></h3>
                <div class="table-responsive">
                  <table class="table">
                      <tr>
                        <td><strong><i class="fa fa-key"></i> Identificador:</strong></td>
                        <td><strong><i class="fa fa-phone"></i> Teléfono:</strong></td>
                        <td><strong><i class="fa fa-home"></i> Dirección:</strong></td>
                        <td><strong><i class="fa fa-calendar-o"></i> Fecha de creación:</strong></td>
                      </tr>
                      <tr>
                        <td><%= @warehouse.hash_id %></td>
                        <td><%= @warehouse.telephone %></td>
                        <td><%= @warehouse.address %></td>
                        <td><%= l(@warehouse.created_at, format: :long) %></td>
                      </tr>
                  </table>
                </div>
              </div>
            </div>
          </div>
      </div>
    </div>
</div><!--<br>-->

<div id="<%= state.id %>">
	<div class="col-lg-3">
		<div class="widget style1 blue-bg">
			<div class="row vertical-align">
				<div class="col-xs-9">
					<b><%= state.name %></b>
				</div>
				<div class="col-xs-3 text-right">
					<%= link_to admin_warehouse_region_path(warehouse_id, state.id), method: :delete, 
						data: { confirm: "Estas seguro(a) de eliminarla?" }, remote: true, class: "white-link" do %>
						<i class="fa fa-trash f-size-18"></i>
					<% end %>
				</div>
			</div>
		</div>
	</div>
</div>
